{
  "name": "sample-spa",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "dev": "node_modules/.bin/webpack-dev-server --config webpack.dev.js --port=3000 --progress",
    "sw": "node_modules/.bin/webpack-dev-server --config webpack.sw.js --port=3000 --progress",
    "build": "rm -rf build && mkdir build && cp -r index.html js/sw.js css fonts img build/ && node_modules/.bin/webpack --config webpack.prod.js -p --progress",
    "test": "mocha --compilers js:babel-core/register ./js/test.js",
    "deploy": "npm run build && ssh cody rm -r /srv/web/sandbox/* && rsync -Pr build/* cody:/srv/web/sandbox/"
  },
  "author": "ccomb",
  "license": "ISC",
  "dependencies": {
    "flexboxgrid": "*",
    "material-ui": "*",
    "react": "*",
    "react-dom": "*",
    "react-grid-layout": "*",
    "react-quill": "*",
    "react-redux": "*",
    "react-tap-event-plugin": "*",
    "redux": "*",
    "redux-thunk": "*",
    "uuid": "*"
  },
  "devDependencies": {
    "babel": "*",
    "babel-core": "*",
    "babel-loader": "*",
    "babel-preset-es2015": "*",
    "babel-preset-react": "*",
    "babel-preset-stage-0": "*",
    "chai": "*",
    "mocha": "*",
    "react-hot-loader": "*",
    "webpack": "*",
    "webpack-dev-server": "*"
  }
}
